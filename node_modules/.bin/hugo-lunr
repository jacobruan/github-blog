#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_hugo-lunr@0.0.4@hugo-lunr/bin/index.js" "$@"
  ret=$?
else 
  node  "$basedir/../_hugo-lunr@0.0.4@hugo-lunr/bin/index.js" "$@"
  ret=$?
fi
exit $ret
